<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fastfoodapp">

    <application
        android:name="com.example.fastfoodapp.FastFoodApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FastFoodApp">
        <activity
            android:name=".Dim4es.SuccessPay.SuccessPayPage"
            android:theme="@style/Theme.Design.NoActionBar"
            />
        <activity
            android:name=".Dim4es.CreditCard.AddFormatCreditCard"
            android:theme="@style/Theme.Design.NoActionBar"
            />
        <activity
            android:name=".Dim4es.PayPage.PayMainActivity"
            android:theme="@style/Theme.Design.NoActionBar" />

        <activity android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.FastFoodApp.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Maxerum.OrderPayingActivity" />

        <activity android:name=".eugene.menu.MenuActivity"
            android:theme="@style/Theme.FastFoodApp.NoActionBar">
        </activity>

        <activity android:name=".eugene.authorization.AuthorizationActivity"
            android:theme="@style/Theme.FastFoodApp.NoActionBar">
        </activity>

        <activity android:name=".eugene.account.AccountActivity"
            android:theme="@style/Theme.FastFoodApp.NoActionBar">
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>